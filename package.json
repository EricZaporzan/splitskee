{
  "name": "mithril-webpack-starter",
  "version": "0.0.1",
  "description": "Mithril + Webpack",
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "watch": "NODE_ENV=development webpack-dev-server --content-base ./dist/ --inline --hot --host=0.0.0.0 --port=9000",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "prebuild": "npm run clean",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "test": "mocha --compilers js:babel-register",
    "lint": "eslint ./src ./spec"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "lodash": "^4.16.4",
    "mithril": "github:lhorie/mithril.js#rewrite"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "baggage-loader": "^0.2.4",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "image-webpack-loader": "^3.0.0",
    "imports-loader": "^0.6.5",
    "jsdom": "9.8.0",
    "jsdom-global": "2.1.0",
    "lost": "^7.1.0",
    "mithril-query": "github:stephanhoyer/mithril-query#rewrite",
    "mocha": "^3.1.2",
    "postcss": "^5.2.5",
    "postcss-cssnext": "^2.8.0",
    "postcss-extend": "^1.0.5",
    "postcss-functions": "^2.1.1",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^1.0.0",
    "rimraf": "^2.5.4",
    "rucksack-css": "^0.8.6",
    "sanitize.css": "^4.1.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
